{
  "version": "2.0",
  "name": "example-1-predict-v2",
  "engineType": "Python",
  "pipelineType": "model_consumer",
  "pipe": {
    "random-dataframe": {
      "componentName": "random-dataframe",
      "parents": [],
      "arguments": {
        "num_cols": 5,
        "num_lines": 10
      }
    },
    "string-src": {
      "componentName": "string-source",
      "parents": [],
      "arguments": {
        "value": "s3:some-value-of-s3-bucket"
      }
    },
    "predict": {
      "componentName": "r-predict-connected",
      "parents": [
        {
          "parent": "random-dataframe"
        },
        {
          "parent": "string-src",
          "output": "string"
        }
      ],
      "arguments": {
        "num_iter": 5
      }
    },
    "string-sink-a": {
      "componentName": "string-sink",
      "parents": [
        {
          "parent": "predict",
          "output": "s1-filename"
        }
      ],
      "arguments": {
        "expected-value": "s1"
      }
    },
    "string-sink-b": {
      "componentName": "string-sink",
      "parents": [
        {
          "parent": "predict",
          "output": "s2-filename"
        }
      ],
      "arguments": {
        "expected-value": "s2"
      }
    }
  }
}
